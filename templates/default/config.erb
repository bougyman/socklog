s<%= @log.size %>
n<%= @log.num_files_max %>
N<%= @log.num_files_min %>
t<%= @log.rotate_seconds %>
<% if @log.remote_syslog -%>
  <% if @log.remote_syslog['remote_only'] -%>
U<%= "%s:%s" % [@log.remote_syslog[:ip], (@log.remote_syslog[:port] || 514)] %>
  <% else -%>
u<%= "%s:%s" % [@log.remote_syslog[:ip], (@log.remote_syslog[:port] || 514)] %>
  <% end -%>
<% end -%>
<% if @log.prefix -%>
p<%= @log.prefix %>
<% end -%>
<% if @log.exclude_patterns -%>
  <% @log.exclude_patterns.each do |pattern| -%>
-<%= pattern %>
  <% end -%>
<% end -%>
<% if @log.include_patterns -%>
  <% @log.include_patterns.each do |pattern| -%>
+<%= pattern %>
  <% end -%>
<% end -%>
<% if @log.include_error_patterns -%>
  <% @log.include_error_patterns.each do |pattern| -%>
e<%= pattern %>
  <% end -%>
<% end -%>
<% if @log.exclude_error_patterns -%>
  <% @log.exclude_error_patterns.each do |pattern| -%>
E<%= pattern %>
  <% end -%>
<% end -%>
<% if @log.post_processor -%>
!<%= @log.post_processor %>
<% end -%>



